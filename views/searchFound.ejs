<!-- views/searchFound.ejs -->
<!doctype html>
<html>
<head>
	<title>ShareIITK-searchFound</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>
<div class="container">

	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> The following items matched your search!</h1>
	</div>

	<div class="row">


    <div>
        <% for ( var i=0; i < itemList.length; i++ ) { %>
            <li>
                <strong>
                    <%= itemList[i].name %><a href="/request" class="btn btn-default"><span class="fa fa-anchor"></span> Request</a> <br>
                </strong>
            </li>
            <% } %>
    </div>
</div>
</body>
</html>
